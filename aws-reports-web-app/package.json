{
  "name": "aws-reports-web-app",
  "version": "1.0.0",
  "description": "AWS reports desktop application",
  "author": "Mohamed AÃ¯t El Kamel",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:webpack": "next dev",
    "build": "next build",
    "build:electron": "ELECTRON_BUILD=true next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf node_modules package-lock.json && npm cache clean --force && npm install",
    "dev:clean": "npm run clean && npm run dev",
    "electron": "electron electron/main.js",
    "electron:dev": "NODE_ENV=development electron electron/main.js",
    "electron:build": "./scripts/build-electron.sh",
    "electron:build:mac": "./scripts/build-electron.sh --mac",
    "electron:build:win": "./scripts/build-electron.sh --win",
    "electron:build:linux": "./scripts/build-electron.sh --linux",
    "electron:build:all": "./scripts/build-electron.sh --mac --win --linux",
    "dist": "npm run build:electron && electron-builder --publish=never"
  },
  "dependencies": {
    "@aws-sdk/client-cost-explorer": "^3.835.0",
    "@aws-sdk/client-securityhub": "^3.835.0",
    "@aws-sdk/client-sts": "^3.835.0",
    "@aws-sdk/credential-providers": "^3.835.0",
    "@tanstack/react-query": "^5.81.2",
    "antd": "^5.26.1",
    "chart.js": "^4.5.0",
    "chartjs-node-canvas": "^5.0.0",
    "dayjs": "^1.11.13",
    "http-proxy-agent": "^7.0.2",
    "https-proxy-agent": "^7.0.6",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "next": "15.3.4",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "xlsx": "^0.18.5",
    "yaml": "^2.8.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "electron": "^37.2.1",
    "electron-builder": "^26.0.12",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "jest": "^30.0.2",
    "playwright": "^1.53.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "build": {
    "appId": "com.awsreports.app",
    "productName": "AWS Reports",
    "directories": {
      "output": "dist"
    },
    "files": [
      ".next/**/*",
      "electron/**/*",
      "finops-cost-report/**/*",
      "securityhub/**/*",
      "public/**/*",
      "node_modules/**/*",
      "package.json",
      "!**/*.ts",
      "!**/*.tsx",
      "!src/**/*",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin"
    ],
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        }
      ],
      "artifactName": "${productName}-${version}-${arch}-setup.${ext}",
      "requestedExecutionLevel": "asInvoker"
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "deb",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "rpm",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "tar.gz",
          "arch": [
            "x64"
          ]
        }
      ],
      "category": "Development",
      "artifactName": "${productName}-${version}-${arch}.${ext}"
    },
    "npmRebuild": false,
    "nodeGypRebuild": false,
    "asarUnpack": [
      "node_modules/@aws-sdk/**/*"
    ],
    "extraMetadata": {
      "dependencies": {}
    }
  }
}